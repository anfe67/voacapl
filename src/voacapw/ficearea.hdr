C
C Header file of ficepac.hdr
C to declare fields for data entry for area coverage Windows version
C


      character(len=6) :: color(6)
      character(len=12) :: cityname
      character(len=6) :: parms(5)                     !  parameters to contour
      real(kind=4) :: conlevels(10,5)                  !  contour levels
      integer(kind=4) :: nparms,nlevels(5),ishades(10,5)   !  shading colors
      character(len=10) :: tlatdeg,tlondeg,platdeg,plondeg
      character(len=20) :: tname,pname
      real(kind=4) :: xmin,xmax,ymin,ymax
      integer(kind=4) :: ngrid,gridtype,method,ipath,icoeffs
      real(kind=4) :: montha(MAX_AREA_MONTHS)
      integer(kind=4) :: ssna(MAX_AREA_MONTHS),ihour(MAX_AREA_MONTHS)
      real(kind=4) :: qindexa(MAX_AREA_MONTHS)
      real(kind=4) :: Freq(MAX_AREA_MONTHS)
      integer(kind=4) :: noise
      real(kind=4) :: amind
      integer(kind=4) :: xlufp,rsn
      real(kind=4) :: pmp,dmpx
      real(kind=4) :: fprob(4)
      character(len=8) :: xmtr_dir
      character(len=12) :: xmtr_file
      character(len=10) :: xmtr_model
      real(kind=4) :: design_freq,beam_main,TxPower
      character(len=8) :: rec_dir
      character(len=12) :: rec_file
      real(kind=4) :: rec_gain,rec_bear

      COMMON /zantenna/ color,cityname           !  reuse /cantenna/
     @    ,nparms,parms,nlevels,conlevels,ishades
     @    ,tlatdeg,tlondeg,tname,platdeg,plondeg,pname
     @    ,xmin,xmax,ymin,ymax
     @    ,ngrid,gridtype,method,ipath,icoeffs
     @    ,montha,ssna,qindexa,ihour,Freq
     @    ,noise,amind,xlufp,rsn
     @    ,pmp,dmpx,fprob
     @    ,xmtr_dir,xmtr_file,xmtr_model,design_freq,beam_main,TxPower
     @    ,rec_dir,rec_file,rec_gain,rec_bear
